<div class="container text-center">
  <h3 class="genre mb-3">ジャンル編集画面</h3>

    <%= form_with model: @genre, url:admin_genre_path do |f| %>
      <div class="field mt-3 mb-4">
        <%= f.text_field :name %>
      </div>
      <div class="actions ">
        <%= f.submit "保存", class:"btn btn-outline-dark mb-4"%>
      </div>
    <% end %>
</div>